<Window x:Class="PQSE_GUI.EditStone"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PQSE_GUI"
        mc:Ignorable="d"
        Title="EditStone" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="saveStone" Click="saveStone_Click" Content="Save Stone" />


            <Label Content="BasicValue: " />
            <TextBox x:Name="txt_stoneVal" />


        </StackPanel>
        
    </Grid>
</Window>
